{
    "browser": "chrome",
    "options":
    {
        "log_JS": 
        {
            "active": true,
            "refresh_rate": 1000,
            "retry_timeout": 1000
        },
        "terminal_mode": true,
        "auto_exit_iframes": true,
        "parent_log_path": "./logs"
    },
    "driver_options":
    {
        "page_load_strategy": "normal",
        "accept_insecure_certs": false,
        "timeout":
        {
            "type": "pageLoad",
            "value": 300000
        },
        "unhandled_prompt_behavior": "dismiss and notify",
        "keep_browser_open": true,
        "browser_arguments": []
    },
    
    "units":
    {
        "1":
        {
            "bindings": "2",
            "actions":
            {
                "1":
                {
                    "type": "goto",
                    "break": false,
                    "url": "https://bestbyte.hu/blog"
                }
            }
        },
        "2":
        {
            "bindings": null,
            "actions":
            {
                "1":
                {
                    "type": "click",
                    "break": false,
                    "single": true,
                    "displayed": null,
                    "enabled": null,
                    "selected": null,
                    "locator": "css_selector",
                    "value": "[id$=\"EDI_search\"]"
                },
                "2":
                {
                    "type": "send_keys",
                    "break": false,
                    "single": true,
                    "displayed": null,
                    "enabled": null,
                    "selected": null,
                    "keys": "ssd\n",
                    "locator": "css_selector",
                    "value": "[id$=\"EDI_search\"]"
                },
                "3":
                {
                    "type": "js_execute",
                    "break": false,
                    "commands":
                    [
                        "console.log('Hello World'",
                        "for(let i = 0; i < 5; i++){console.log(i)}"
                    ]
                }
            }
        }
    }
}