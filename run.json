{
    "browser": "chrome",
    "options":
    {
        "log_JS": 
        {
            "active": true,
            "path": "./JS.log",
            "create_path": true,
            "terminal_mode": true,
            "refresh_rate": 1000,
            "retry_timeout": 1000
        }
    },
    "driver_options":
    {
        "options":
        {
            "page_load_strategy": "normal",
            "accept_insecure_certs": false,
            "timeout":
            {
                "type": "pageLoad",
                "value": 300000
            },
            "unhandled_prompt_behavior": "dismiss and notify",
            "keep_browser_open": true,
            "browser_arguments": []
        },
        "service":
        {
            "log_path": "./service.log",
            "arguments": []
        }
    },
    
    "actions":
    {
    "1":
        {
            "type": "goto",
            "break": false,
            "url": "https://wikipedia.org"
        },
    "2":
        {
            "type": "js_execute",
            "break": false,
            "commands":
            [
                "console.log('Hello Wolrd!')",
                "for(let i = 0; i < 5; i++){console.log(i);}"
            ]
        },
    "3":
        {
            "type": "wait",
            "break": false,
            "amount": 1000
        },
    "4":
        {
            "type": "js_execute",
            "break": false,
            "commands":
            [
                "for(let i = 0; i < 5; i++){console.log(i);}"
            ]
        }
    }
}