{
    "browser": "chrome",
    "options":
    {
        "log_JS": 
        {
            "active": true,
            "path": "./JS.log",
            "create_path": true,
            "terminal_mode": true,
            "refresh_rate": 1000,
            "retry_timeout": 1000
        }
    },
    "driver_options":
    {
        "options":
        {
            "page_load_strategy": "normal",
            "accept_insecure_certs": false,
            "timeout":
            {
                "type": "pageLoad",
                "value": 300000
            },
            "unhandled_prompt_behavior": "dismiss and notify",
            "keep_browser_open": true,
            "browser_arguments": []
        },
        "service":
        {
            "log_path": ".",
            "arguments": []
        }
    },
    
    "units":
    {
        "1":
        {
            "binding": null,
            "actions": {}
        }
    }
}