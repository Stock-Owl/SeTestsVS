{
    "browser": "chrome",
    "options":
    {
        "log_JS": 
        {
            "active": true,
            "path": "./JS.log",
            "refresh_rate": 1000,
            "retry_timeout": 1000
        }
    },
    "driver-options":
    {
        "page_load_startegy": "normal",
        "accept_insecure_certs": false,
        "timeout":
        {
            "type": "pageLoad",
            "value": 300000
        },
        "unhandled_prompt_behavior": "dismiss and notify",
        "keep_browser_open": true,
        "browser_args": [],
        "service_log_path": ".",
        "service_args": []
    },
    
    "actions":
    {
    }
}