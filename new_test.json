{
    "browser": "chrome",
    "options":
    {
        "log_JS": 
        {
            "active": true,
            "refresh_rate": 1000,
            "retry_timeout": 1000
        },
        "terminal_mode": true,
        "exception_log_path": "./exceptions.log",
        "auto_exit_iframes": true,
        "parent_log_path": "./logs"
    },
    "driver_options":
    {
            "page_load_strategy": "normal",
            "accept_insecure_certs": false,
            "timeout":
            {
            "type": "pageLoad",
            "value": 300000
            },
            "unhandled_prompt_behavior": "dismiss and notify",
            "keep_browser_open": true,
            "browser_arguments": [],
    "service":
        {
            "log_path": "./service.log",
            "arguments": []
        }
    },
    "units":
    {
        "1":
        {
            "bindings": null,
            "backup_of": null,
            "actions":
            {
                "1":
                {
                    "type": "goto",
                    "break": false,
                    "url": "https://youtube.com"
                }
            }
        }
    }
}